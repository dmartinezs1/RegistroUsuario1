<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>GetServlet</servlet-name>
        <servlet-class>co.edu.sena.RegistroUsuario.controller.GetServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetServlet</servlet-name>
        <url-pattern>/getServlet</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>registroServlet</servlet-name>
        <servlet-class>co.edu.sena.RegistroUsuario.controller.ServletRegistro</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>registroServlet</servlet-name>
        <url-pattern>/registro</url-pattern>
    </servlet-mapping>
</web-app>
